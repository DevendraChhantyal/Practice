@model IEnumerable<Sample.Application.Models.EmailTemplate>
@{
    ViewBag.Title = "Index";
}
<h2>EmailTemplate</h2>
<div class="pull-right">
    <p>
      <a href="/EmailTemplate/Create" class="btn btn-success btn-xs">Register<span class="glyphicon glyphicon-plus"> </span></a></p>
</div>
<table class="table table-striped table-bordered table-hover">

    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Content</th>
        <th>AddedDate</th>
        <th>Status</th>
        <th>Action</th>
    </tr>


    @foreach (var m in Model)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.Title</td>
            <td>@m.Content</td>
            <td>@m.AddedDate</td>
            <td>@m.Status</td>

            <td>
                @Html.ActionLink("Edit", "Edit/" + @m.Id, "EmailTemplate")
                @Html.ActionLink("Delete", "Delete/" + @m.Id, "EmailTemplate")
            </td>

        </tr>
    }
</table>